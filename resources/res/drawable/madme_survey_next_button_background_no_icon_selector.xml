<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/madme_survey_next_button_disabled_background_color_no_icone"/>
            <stroke android:width="@dimen/madme_survey_next_button_stroke_size" android:color="@color/madme_survey_next_button_outline_color_no_icon"/>
            <corners android:radius="@dimen/madme_survey_next_button_corners"/>
            <padding android:left="@dimen/madme_survey_next_button_padding" android:top="0dp" android:right="@dimen/madme_survey_next_button_padding" android:bottom="0dp"/>
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape>
            <solid android:color="@color/madme_survey_next_button_enabled_background_color_no_icone"/>
            <stroke android:width="@dimen/madme_survey_next_button_stroke_size" android:color="@color/madme_survey_next_button_outline_color_no_icon"/>
            <corners android:radius="@dimen/madme_survey_next_button_corners"/>
            <padding android:left="@dimen/madme_survey_next_button_padding" android:top="0dp" android:right="@dimen/madme_survey_submit_button_padding" android:bottom="0dp"/>
        </shape>
    </item>
</selector>
