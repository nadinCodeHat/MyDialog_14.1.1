<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#ffffff"/>
        </shape>
    </item>
    <item android:left="-3dp" android:top="-3dp" android:right="-3dp">
        <inset android:insetLeft="-6dp" android:insetRight="-6dp">
            <shape>
                <solid android:color="@android:color/transparent"/>
                <stroke android:width="2dp" android:color="@color/Smooch_accent"/>
            </shape>
        </inset>
    </item>
</layer-list>
