<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/madme_survey_prev_button_disabled_background_color"/>
            <stroke android:width="@dimen/madme_survey_prev_button_stroke_size" android:color="@color/madme_survey_prev_button_outline_color"/>
            <corners android:radius="@dimen/madme_survey_prev_button_corners"/>
            <padding android:left="0dp" android:top="0dp" android:right="0dp" android:bottom="0dp"/>
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape>
            <solid android:color="@color/madme_survey_prev_button_enabled_background_color"/>
            <stroke android:width="@dimen/madme_survey_prev_button_stroke_size" android:color="@color/madme_survey_prev_button_outline_color"/>
            <corners android:radius="@dimen/madme_survey_prev_button_corners"/>
            <padding android:left="0dp" android:top="0dp" android:right="0dp" android:bottom="0dp"/>
        </shape>
    </item>
</selector>
